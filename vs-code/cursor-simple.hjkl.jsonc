/* CURSOR */
[
  {
    "key": "alt+h",
    "command": "cursorLeft",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+j",
    "command": "cursorDown",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+k",
    "command": "cursorUp",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+l",
    "command": "cursorRight",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+shift+h",
    "command": "cursorLeftSelect",
    "when": "editorTextFocus"
  },
  {
    "command": "cursorDownSelect",
    "key": "alt+shift+j",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+shift+k",
    "command": "cursorUpSelect",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+shift+l",
    "command": "cursorRightSelect",
    "when": "editorTextFocus"
  }
]